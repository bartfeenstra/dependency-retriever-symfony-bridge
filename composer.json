{
  "name": "bartfeenstra/dependency-retriever-symfony-bridge",
  "license": "MIT",
  "require": {
    "bartfeenstra/dependency-retriever": ">=0.3.2 <0.4.0",
    "symfony/dependency-injection": "~2.8 || ~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.2",
    "squizlabs/php_codesniffer": "~2.5"
  },
  "autoload": {
    "psr-4": {
      "BartFeenstra\\DependencyRetrieverSymfonyBridge\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BartFeenstra\\DependencyRetrieverSymfonyBridge\\Tests\\": "tests"
    }
  }
}
